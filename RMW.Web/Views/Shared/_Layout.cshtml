    <div class="header-container">
        <header class="wrapper clearfix">
        
            <h1 class="title">
                <a href="~/" >RMW</a> </h1>
            <nav>
                <ul>
             


                    
                       @if ((ViewContext.RouteData.Values["controller"].ToString().ToLower() == "articles" &&
                            ViewContext.RouteData.Values["action"].ToString().ToLower() == "index"))
                       {
             
                               <li>@Html.ActionLink("Articles", "Index", "Articles", null, new { @class="shadowing active_menu" })</li> 
                       }
                    else 
                    {
                             <li>@Html.ActionLink("Articles", "Index", "Articles", null, new { @class="shadowing" })</li>
                       }



                             @if ((ViewContext.RouteData.Values["controller"].ToString().ToLower() == "home" &&
                            ViewContext.RouteData.Values["action"].ToString().ToLower() == "resume"))
                       {
             
                            
                                    <li>@Html.ActionLink("Résumé", "Resume", "Home", null, new { @class="shadowing active_menu" })</li>
                       }
                    else 
                    {
                       <li>@Html.ActionLink("Résumé", "Resume", "Home", null, new { @class="shadowing" })</li>
                       }

                

                    
                       @if ((ViewContext.RouteData.Values["controller"].ToString().ToLower() == "contact" &&
                            ViewContext.RouteData.Values["action"].ToString().ToLower() == "contact"))
                       {
             
                             <li>@Html.ActionLink("Contact", "Contact", "Home", null, new { @class="shadowing active_menu" })</li>   
                       }
                    else 
                    {
                           <li>@Html.ActionLink("Contact", "Contact", "Home",  null, new { @class="shadowing" })</li>   
                    }
                 

                 
                </ul>
            </nav>
        </header>
    </div>

    <div class="main-container">
        <div class="main wrapper clearfix">
            @RenderSection("featured", required: false)
            <article>
               
                <section>
                    @RenderBody()

                </section>
              
            </article>

            <aside>
                <div class="side_bg">
         
                    <h2 class="home_title">Ryan Williams</h2>
                    <p class="no_margin"><b>Full Stack Web Developer</b></p>
                    <a href="~/home/resume" class="h_img"><img src="~/content/img/me.jpg" alt="About Ryan Williams" title="About Ryan Williams" /></a>
             
                    <ul>
                        <li>ASP.NET/ C#</li>
                        <li>Azure</li>
                        <li>SQL Server</li>
                        <li>HTML5, CSS, JavaScript</li>

      
                    </ul>
  
                    <hr />
                    <p><b>Location</b></p>
                    San Francisco, California  
                    <img src="~/Content/img/US_small.png" />

                    <hr />
                    
                    <p><b>Open Source Work</b></p> 
                    <a   target="_blank" href="https://github.com/ryn0">http://github.com/ryn0</a>   
        
                </div>
            </aside>
            
            <aside style="margin-top:20px;">
                <h4>Advertisement</h4>
             
                
                <div style="text-align:center;">
                    <script type="text/javascript">
                        amzn_assoc_ad_type = "banner";
                        amzn_assoc_marketplace = "amazon";
                        amzn_assoc_region = "US";
                        amzn_assoc_placement = "assoc_banner_placement_default";
                        amzn_assoc_banner_type = "ez";
                        amzn_assoc_p = "9";
                        amzn_assoc_width = "180";
                        amzn_assoc_height = "150";
                        amzn_assoc_tracking_id = "dkm543-20";
                        amzn_assoc_linkid = "844c47524044abfe9765d19c2ff341ad";
                    </script>
                    <script src="//z-na.amazon-adsystem.com/widgets/q?ServiceVersion=20070822&Operation=GetScript&ID=OneJS&WS=1"></script>
                </div>
               
            </aside>
        </div>
        <!-- #main -->
    </div>
    <!-- #main-container -->

    <div class="footer-container">
 

          <footer class="wrapper">
                    <p>&copy; @DateTime.Now.Year - Ryan Williams | @Html.Partial("_LoginPartial") </p> 
                </footer>
    </div>

